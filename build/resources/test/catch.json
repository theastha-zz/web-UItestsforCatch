  "browser": {
    "browserName": "chrome",
    "version": "",
    "platformName": "ANY",
    "launch_configuration": {
      "maximize": false,
      "headless": true,
      "implicit_wait": 7,
      "download_path": "/tmp/download",
      "log_level": "DEBUG"
    }
  }
}
